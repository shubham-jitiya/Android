<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:parentTag="androidx.constraintlayout.widget.ConstraintLayout">

    <TextView
        android:id="@+id/textViewTitleShouldKnow"
        style="@style/GuestLocationDetailsInfoTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="33dp"
        android:text="@string/things_you_should_know"
        app:layout_constraintTop_toBottomOf="@id/editTextEnterAfterTime" />

    <TextView
        android:id="@+id/textViewDescriptionShouldKnow"
        style="@style/GuestLocationDetailsDescription"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/should_know_list"
        app:layout_constraintTop_toBottomOf="@id/textViewTitleShouldKnow" />
</merge>